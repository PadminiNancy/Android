<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geolite"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.geolite.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.geolite.PinSuccess"
            android:label="@string/title_activity_pin_success" >
        </activity>
        <activity
            android:name="com.example.geolite.Accounts"
            android:label="@string/title_activity_accounts" >
        </activity>
        <activity
            android:name="com.example.geolite.SetPassword"
            android:label="@string/title_activity_set_password" >
        </activity>
        <activity
            android:name="com.example.geolite.YourInfo"
            android:label="@string/title_activity_your_info" >
        </activity>
        <activity
            android:name="com.example.geolite.Password"
            android:label="@string/title_activity_password" >
        </activity>
        <activity
            android:name="com.example.geolite.UpdatePassword"
            android:label="@string/title_activity_update_password" >
        </activity>
        <activity
            android:name="com.example.geolite.PinConfirmation"
            android:label="@string/title_activity_pin_confirmation" >
        </activity>
    </application>

</manifest>
